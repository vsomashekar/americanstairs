<?xml version='1.0' encoding='UTF-8'?>  
    <!DOCTYPE hibernate-mapping PUBLIC  
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
      
     <hibernate-mapping>
      <class name="com.american.stairs.model.User" table="americanstairsdb.users">
        <id name="UserId">  
         <generator class="assigned"></generator>  
        </id>  
 
        <property name="UserName" column="UserName"></property>     
        <property name="Email" column="Email"></property>  
        <property name="Password" column="Password"></property>  
		<property name="RoleId" column="RoleId"></property>  
		<property name="IsActive" column="IsActive"></property>  
		<property name="Created" column="Created" type="timestamp" ></property>              
      </class>  
                
     </hibernate-mapping>  